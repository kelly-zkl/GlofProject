
.weui-grid__icon{display:block;width:50px;height:50px;margin:0 auto}
.weui-flex__item{
  position: relative
}

.weui-check{
  position: absolute;
  top: -15%;
  left: 10%
}

.weui-cell{
  height:30px;
  min-height:30px;
  line-height:30px
}
.weui-grid__icon.s{display:inline-block;width:30px;height:30px;margin:auto 5px}

.tui-gallery-list{
  position: relative;
  height:30px;
  line-height: 30px;
  text-align: center;
}
.tui-number-group{
  display: table;
  table-layout: fixed;
  height:30px;
  line-height: 30px;
  text-align: center;
  width: 140px;
  border-radius: 17px;
  border: 1px solid #66CC99;
  overflow: hidden;
  color: #66CC99
}
.tui-number-cell{
  display: table-cell;
  height:30px;
  text-align: center;
  line-height: 30px;
  border-radius: 0;
  font-size: 14px;
}
.tui-number-cell.in{
  height:30px;
  text-align: center;
  line-height: 30px;
  min-height: 30px;
}

.zan-popup__container{
  border-radius: 8px;
  width: 90%;
}
.popup-width {
  background: #fff;
  border-radius: 8px;
  padding: 15px;
}