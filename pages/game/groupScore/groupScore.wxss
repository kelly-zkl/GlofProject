
page{
  background: #fff
}

.tag{
  color: #fff;
  font-size: 14px;
  width: 40px;
  text-align: center;
  white-space: pre-line;
  line-height:15px;
}
.tag.hole{
  background: #66CC99;
  padding-top: 7px
}
.tag.rar{
  background: #66CCFF;
  padding-top: 15px
}

.cell{
  color:#fff;font-size:14px;text-align:center
}

.tui-gallery-list{
  color: #555;
  font-size: 35rpx;
  position: relative;
  margin-top: 10px;
}
.tui-number-group{
  display: table;
  table-layout: fixed;
  width: 135px;
  text-align: center;
  border-radius: 4px;
  border: 1px solid #bbb;
  overflow: hidden;
}
.tui-number-cell{
  display: table-cell;
  height:30px;
  line-height: 30px;
  border-radius: 0;
  text-align: center;
  font-size: 18px;
}
.tui-number-cell.in{
  height:30px;
  line-height: 30px;
  font-size: 14px;
  text-align: center;
  border-left: 1rpx solid #bbb;
  border-right: 1rpx solid #bbb;
}

.hole-on{
  border-right: 3px solid #66CC99;
  border-left: 3px solid #66CC99;
  text-align: center
}
.hole-off{
  border-right: none;
  border-left: none;
  text-align: center
}
.weui-grid__label.hole-on{
  color: #66CC99;
  font-size: 16px;
  border-right: none;
  border-left: none;
}
.weui-grid__label.hole-off{
  color: #bbb;
  font-size: 13px;
  border-right: none;
  border-left: none;
}