.tui-tabbar-group{
  border: 1rpx #66CC99 solid;
  border-radius: 30rpx;
  overflow: hidden;
  display: inline-block;
  color: #66CC99;
}
.tui-tabbar-cell{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 14px;
  padding-left: 20rpx;
  padding-right: 20rpx;
  text-align: center;
}
.tui-tabbar-cell:not(:last-child){border-right: 1rpx solid #66CC99;}
.tui-tabbar-cell.tui-active{background-color: #66CC99;color: #fff;}