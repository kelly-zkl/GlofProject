.weui-media-box__thumb{
  width: 60px;
  height: 60px;
  border-radius: 10rpx;
  vertical-align:center
}

.weui-panel:before{
  display: none
}
.weui-flex{
  background:#ffffff;
}
.weui-flex__item{
  border-right: 1rpx solid #E5E5E5
}
.weui-flex__item:last-child{
  border-right: none
}

/**二维码*/
.popup-example--center {
  border-radius: 4px;
  width: 80%;
}
.popup-width{
  padding-top: 20rpx;
  padding-bottom: 50px
}
.weui-flex.score{
  border-radius:0 0 4px 4px;
}
.weui-grid__label.score{
  margin-top: 0;
  padding: 15rpx 0;
  background: transparent
}