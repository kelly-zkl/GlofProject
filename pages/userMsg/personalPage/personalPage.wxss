.weui-panel:before,.weui-panel:after{
  display: none
}

.msg-info{
  padding:30rpx;
  font-size:13px;
  color:#999999;
  border-top: 1rpx #e5e5e5 solid;
  background: #fff;
  line-height:1em;
  list-style:none;
  overflow:hidden
}

.weui-media-box__thumb.d1{
  width: 40px;
  height: 40px;
}
.weui-media-box__info.d1{
  margin-top:0;padding-bottom:0;
}
.weui-media-box__hd_in-appmsg.d1{margin-right:8px;width:40px;height:40px;line-height:40px;text-align:center}
.weui-panel__ft{
  margin: 0 0 0 48px;
  padding-top: 10rpx;
}
.weui-media-box.d1{padding-bottom:0}
.weui-flex.p1{
  background:#ffffff;
  border-bottom: 1rpx solid #E5E5E5;
  border-top: 1rpx solid #E5E5E5;
}
.weui-flex__item.p1{
  border-right: 1rpx solid #E5E5E5
}
.weui-flex__item:last-child{
  border-right: none
}
.weui-uploader__file{float:left;margin-right:4px;margin-bottom:4px}
.weui-uploader__img{display:block;width:84px;height:84px}
.weui-uploader__input-box{margin-right:4px;margin-bottom:4px;width:82px;height:82px;}
